{
  "short_name": "TGLFS",
  "name": "Telegram Large File Storage",
  "icons": [
    {
        "src": "/icons/TGLFS logo 192.png",
        "type": "image/png",
        "sizes": "192x192"
    },
    {
        "src": "/icons/TGLFS logo 512.png",
        "type": "image/png",
        "sizes": "512x512"
    }
  ],
  "id": "/",
  "start_url": "/",
  "background_color": "#ffffff",
  "display_override": ["window-controls-overlay"],
  "display": "standalone",
  "scope": "/",
  "theme_color": "#24a1de",
  "description": "Telegram large file storage",
  "dir": "auto",
  "lang": "en",
  "categories": [
    "photo",
    "security",
    "utilities"
  ],
  "screenshots": [
    {
        "src": "/screenshots/android_ui.jpg",
        "type": "image/jpeg",
        "sizes": "895x1280",
        "platform": "android"
    }
  ],
  "orientation": "portrait",
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "files",
          "accept": [
            "image/*",
            "video/*",
            "audio/*",
            "application/pdf",
            "application/zip",
            "application/octet-stream",
            "text/plain"
          ]
        }
      ]
    }
  },
  "related_applications": [
    {
        "platform": "play",
        "url": "https://play.google.com/store/apps/details?id=com.kratosle.unlim&hl=en_US"
    }
  ],
  "prefer_related_applications": false
}
